{
  "name": "css-to-rnstyle",
  "displayName": "css-to-rnstyle",
  "publisher": "2333Ge",
  "description": "css-to-rnstyle",
  "version": "0.0.6",
  "repository": "https://github.com/2333Ge/css-to-rnstyle",
  "icon": "source/logo.png",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "keywords": [
    "rnstyle",
    "react native style",
    "rn",
    "css to rnstyle",
    "css style"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-to-rnstyle.convertCssToRnStyle",
        "title": "转换CSS为RN style"
      },
      {
        "command": "css-to-rnstyle.convertRnStyleToCss",
        "title": "转换RN style为CSS"
      },
      {
        "command": "css-to-rnstyle.convertWord",
        "title": "将选中元素在小驼峰、下划线、中划线间转换"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+9",
        "mac": "ctrl+9",
        "command": "css-to-rnstyle.convertCssToRnStyle",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+8",
        "mac": "ctrl+8",
        "command": "css-to-rnstyle.convertRnStyleToCss",
        "when": "editorTextFocus"
      },
      {
        "key": "ctrl+-",
        "mac": "ctrl+-",
        "command": "css-to-rnstyle.convertWord",
        "when": "editorTextFocus"
      }
    ],
    "configuration": [
      {
        "title": "%extension-title%",
        "properties": {
          "css-to-rnstyle.delete-if-not-rn-keys": {
            "type": "boolean",
            "description": "%delete-if-not-rn-keys%",
            "default": true
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "./bin/build.sh"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
